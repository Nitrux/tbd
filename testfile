@ echo
    + echo
    + echo good

@ sh
    + sh -c 'set a b c d; for x; do echo $x; done'
    + sh -c 'cd $HOME; echo $PWD'

@ test
    + test .
    + test -d /
    + test -e .
    + test -e "$HOME"
    + test 2 = 2
    + test 1 -le 1
    + test 2 -gt 1

@ other
    + \> file
    + rm file
    - \< no
